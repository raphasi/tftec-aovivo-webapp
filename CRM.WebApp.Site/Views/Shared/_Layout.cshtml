    <script>
        document.addEventListener("DOMContentLoaded", function () {
            var ctx = document.getElementById('chartBig1').getContext('2d');
            var chart = new Chart(ctx, {
                type: 'line',
                data: {
                    labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'Setember', 'October', 'November', 'December'],
                    datasets: [{
                        label: 'Performance',
                        backgroundColor: 'rgba(0, 123, 255, 0.2)',
                        borderColor: 'rgba(0, 123, 255, 1)',
                        data: [0, 10, 5, 2, 20, 30, 45]
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        x: {
                            display: true,
                            title: {
                                display: true,
                                text: 'Month'
                            }
                        },
                        y: {
                            display: true,
                            title: {
                                display: true,
                                text: 'Value'
                            }
                        }
                    }
                }
            });
        });
    </script>

    <script>
        $(document).ready(function () {
            $('#eventSelect').select2({
                ajax: {
                    url: function () {
                        return $(this).data('url');
                    },
                    dataType: 'json',
                    delay: 250,
                    data: function (params) {
                        return {
                            query: params.term || null // Envia uma string vazia para buscar os registros mais recentes
                        };
                    },
                    processResults: function (data) {
                        return {
                            results: $.map(data, function (item) {
                                return {
                                    id: item.eventID,
                                    text: item.name // ou qualquer outro campo que você queira exibir
                                };
                            })
                        };
                    },
                    cache: true
                },
                minimumInputLength: 0, // Permite abrir o dropdown sem digitar
                placeholder: 'Selecione uma opção',
                allowClear: true
            });

            // Carregar os 10 registros mais recentes ao abrir o dropdown
            $('#eventSelect').on('select2:open', function () {
                var select2 = $(this).data('select2');

                select2.trigger('query', {
                    term: null // Envia uma string vazia para buscar os registros mais recentes
                });

            })
        });
    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
